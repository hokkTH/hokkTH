local DiscordLib = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/discord%20lib.txt")()

local win = DiscordLib:Window("Anime-Warriors-Simlatotr2")

local serv = win:Server("HOKHUB", "")

local tgls = serv:Channel("Main")
local Au = serv:Channel("Auto-Up-Warriors")
local Mic = serv:Channel("mic")

tgls:Toggle("Auto-Farm",false, function(bool)
    _G.Auto = bool
end)
tgls:Toggle("Auto-Click",false,function(bool)
    _G.click = bool
end)
tgls:Toggle("Auto-Collect",false,function(bool)
    _G.Collect = bool
end)
wa = {}
for i,v in pairs(game:GetService("Workspace").ClientEnemies:GetChildren()) do
    if v.HumanoidRootPart:FindFirstChild("EnemyHealthBar") then
        table.insert(wa,v.HumanoidRootPart.EnemyHealthBar.Title.Text)
    end
end
local drop = tgls:Dropdown("Select-Mon",wa, function(bool)
    _G.mon = bool
end)

tgls:Button("RefedMon", function()
    drop:Clear()
    for i,v in pairs(game:GetService("Workspace").ClientEnemies:GetChildren()) do
        if v.HumanoidRootPart:FindFirstChild("EnemyHealthBar") then
            drop:Add(v.HumanoidRootPart.EnemyHealthBar.Title.Text)
        end
    end
end)
Au:Toggle("Auto-Up",false,function(bool)
    _G.au = bool
end)
Warriors = {"REwarriors"}
local AU = Au:Dropdown("Select-Warriors",Warriors, function(bool)
    _G.Select = bool
end)
Au:Button("RefedWarriors", function()
    AU:Clear()
    if game:GetService("Players").LocalPlayer.PlayerGui.Frames.Warriors.Enabled == false then
        for _,e in pairs(getconnections(game:GetService("Players").LocalPlayer.PlayerGui.Main.Left.Buttons.Top.Warriors.Inner.Button.MouseButton1Click)) do
            e.Function()
        end
    end
    wait(0.1)
    for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.Frames.Warriors.Frame.Container:GetDescendants()) do
        if v.Name == "Text" then
            AU:Add(v.Text)
        end
    end
end)
Mic:Button("ReJoin",function()
    game:GetService("TeleportService"):Teleport(game.PlaceId, game:GetService("Players").LocalPlayer)
end)
spawn(function()
    while wait() do
        if _G.click then
            pcall(function()
                if game:GetService("Players").LocalPlayer.PlayerGui.Main.Bottom.Click.Click.Inner.BackgroundTransparency == 0 then
                    local args = {
                        [1] = {
                            [1] = {
                                [1] = "&"
                            }
                        }
                    }
                    game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
                end
            end)
        end
    end
end)
spawn(function()
    while wait() do
        pcall(function()
            if _G.Collect then
                wait(0.2)
                if game:GetService("Workspace").Drops.DataCost ~= 4 then
                    for i,v in pairs(game:GetService("Workspace").Drops:GetChildren()) do
                       game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
                    end
                end
            end
        end)
    end
end)
spawn(function()
    while wait() do
        if _G.Auto then
            pcall(function()
                if game:GetService("Players").LocalPlayer.PlayerGui.Main.Bottom.Click.Click.Inner.BackgroundTransparency == 0.5 then
                    for i,v in pairs(game:GetService("Workspace").ClientEnemies:GetChildren()) do
                        if v.HumanoidRootPart:FindFirstChild("EnemyHealthBar") then
                            if v.HumanoidRootPart.EnemyHealthBar.Title.Text == _G.mon then
                                _G.dd = v
                            elseif v.HumanoidRootPart.EnemyHealthBar.Title.Text == "Giant ".._G.mon then
                                _G.dd = v
                            elseif v.HumanoidRootPart.EnemyHealthBar.Title.Text == "Tiny ".._G.mon then
                                _G.dd = v
                            elseif v.HumanoidRootPart.EnemyHealthBar.Title.Text == "Shiny ".._G.mon then
                                _G.dd = v
                            end
                        end
                    end
                    wait(0.7)
                    local args = {
                        [1] = {
                                [1] = {
                                    [1] = "%",
                                    [2] = tostring(_G.dd)
                                }
                            }    
                        }
                    game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
                    wait(0.7)
                    for r,t in pairs(game:GetService("Workspace").Warriors[game.Players.LocalPlayer.Name]:GetChildren()) do
                        t.HumanoidRootPart.CFrame = _G.dd.HumanoidRootPart.CFrame
                    end
                end
            end)    
        end
    end
end)
spawn(function()
    while wait() do
        local GC = getconnections or get_signal_cons
        if GC then
            for i,v in pairs(GC(game.Players.LocalPlayer.Idled)) do
                if v["Disable"] then
                    v["Disable"](v)
                elseif v["Disconnect"] then
                    v["Disconnect"](v)
                end
            end
        else
            print("lol bad exploit")
            local vu = game:GetService("VirtualUser")
            game:GetService("Players").LocalPlayer.Idled:connect(function()
                vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
                wait(1)
                vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
            end)
        end
    end
end)
local Open = false
game:GetService("UserInputService").InputBegan:Connect(function(KeyCode)
	if KeyCode.KeyCode == Enum.KeyCode.RightControl then
		if Open then
            game:GetService("CoreGui").Discord.MainFrame.Visible = true
            Open =false
		else
		    Open = true
            game:GetService("CoreGui").Discord.MainFrame.Visible = false
		end
	end
end)

spawn(function()
    while wait() do
        if _G.au then
            pcall(function()
                if game:GetService("Players").LocalPlayer.PlayerGui.Frames.Warriors.Info.Visible == false then
                    if game:GetService("Players").LocalPlayer.PlayerGui.Frames.Warriors.Feed.Visible == false then
                        for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.Frames.Warriors.Frame.Container:GetDescendants()) do
                            if v.Name == "Text" then
                                if v.Text ==  _G.Select then
                                    for _,d in pairs(v.Parent:GetChildren()) do
                                        if d.Name == "Button" then
                                           for r,t in pairs(getconnections(d.MouseButton1Click)) do
                                               t.Function()
                                           end
                                        end
                                    end
                                end
                            end
                        end
                    end
                end
                if game:GetService("Players").LocalPlayer.PlayerGui.Frames.Warriors.Info.Visible == true then
                    wait(0.1)
                    for d,o in pairs(getconnections(game:GetService("Players").LocalPlayer.PlayerGui.Frames.Warriors.Info.Buttons.Feed.Inner.Button.MouseButton1Click)) do
                        o.Function()
                    end
                end
                if game:GetService("Players").LocalPlayer.PlayerGui.Frames.Warriors.Feed.Visible == true then
                    for d,o in pairs(getconnections(game:GetService("Players").LocalPlayer.PlayerGui.Frames.Warriors.Feed.Buttons.SelectUnlocked.Inner.Button.MouseButton1Click)) do
                        o.Function()
                    end
                    wait(0.1)
                    for i,v in pairs(getconnections(game:GetService("Players").LocalPlayer.PlayerGui.Frames.Warriors.Feed.Buttons.Feed.Inner.Button.MouseButton1Click)) do
                        v.Function()
                    end
                    wait(0.1)
                    for p,c in pairs(getconnections(game:GetService("Players").LocalPlayer.PlayerGui.Components.Confirm.Frame.Yes.Button.MouseButton1Click)) do
                        c.Function()
                    end
                end
                wait(5)
            end)
        end
    end
end)
spawn(function()
    while wait() do
        if _G.au then
            pcall(function()
                local args = {
                    [1] = "Reaper",
                    [3] = false
                }
                
                game:GetService("ReplicatedStorage").Packages._Index:FindFirstChild("sleitnick_knit@1.4.7").knit.Services.EggService.RF.Open:InvokeServer(unpack(args))
                wait(0.5)
            end)
        end
    end
end)
