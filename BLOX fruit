local Remote_GetFruits = game.ReplicatedStorage:FindFirstChild("Remotes").CommF_:InvokeServer("GetFruits");
Table_DevilFruitSniper = {}
ShopDevilSell = {}
for i,v in next,Remote_GetFruits do
    table.insert(Table_DevilFruitSniper,v.Name)
    if v.OnSale then 
        table.insert(ShopDevilSell,v.Name)
    end
end
local Tab4 = GUI:Tab{
    Name = "Fruit",
    Icon = "rbxassetid://7044284832"
}
SelectDevil = ""
local opdown = Tab4:Dropdown{
    Name = "SelectDeviFruit",
    StartingText = "Select...",
    Description = nil,
    Items = Table_DevilFruitSniper,
    Callback = function(dv) 
        _G.SelectDevil = dv
        return
    end
}
Tab4:Toggle{
    Name = "Buy Devil Fruit Sinper",
    StartingState = _G.DevilFruitSinper,
    Description = nil,
    Callback = function(buy)
        BuyFruitSinper = buy
    end
}
spawn(function()
    while wait(.1) do
        if BuyFruitSinper then
            local args = {
                [1] = "GetFruits"
            }
            
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
            local args = {
                [1] = "PurchaseRawFruit",
                [2] = _G.SelectDevil
            }
            
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
        end 
    end
end)
Tab4:Toggle{
    Name = "AutoRandomFruit",
    StartingState = _G.AutoRandomFruit,
    Description = nil,
    Callback = function(RF)
        _G.RF = RF
    end
}
Tab4:Toggle{
    Name = "BringFruit",
    StartingState = _G.BringFruit,
    Description = nil,
    Callback = function(BFt)
        _G.BFt = BFt
    end
}
spawn(function()
    while wait() do
        if _G.BFt then
            for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
                if v:IsA("Tool") and string.find(v.Name,"Fruit") then wait(5)
                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart,v.Handle,0)    
                end
            end
        end
    end
end)
