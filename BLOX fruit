    if type(v)=="table" then
        if rawget(v,"attack") then
                _G.Attack = rawget(v,"attack")
        end
        if rawget(v,"increment") and rawget(v,"equipped") then
            _G.p13 = v
        end
    end
end



local SelectMob 
local a = _G.Attack



local u7 = debug.getupvalues(_G.Attack)[4]
local u8 = debug.getupvalues(_G.Attack)[5]
local u9 = debug.getupvalues(_G.Attack)[6]
local u10 = debug.getupvalues(_G.Attack)[7]

function fastattack()
local Fast = {}
    for i, v in pairs(game:GetService("CollectionService"):GetTagged("ActiveRig")) do
        if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Name ~= game.Players.LocalPlayer.Name and (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude <= 250 then
            p = v.HumanoidRootPart
        table.insert(Fast, v.HumanoidRootPart) 
        end
end

-- print(u7,u8,u9,u10,"\n")
    local u12 = (u8 * 798405 + u7 * 727595) % u9;
        local u13 = u7 * 798405;
        
            u12 = (u12 * u9 + u13) % 1099511627776;
            u8 = math.floor(u12 / u9);
            u7 = u12 - u8 * u9;
    
        u10 = u10 + 1;
          local Animate =  Instance.new("Animation")
        Animate.AnimationId = RigC.activeController.anims.basic[1]
        game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(Animate):Play(0.1,2,1)
        game.ReplicatedStorage.Remotes.Validator:FireServer(math.floor(u12 / 1099511627776 * 16777215), u10);

        -- print(#Fast)
        game:GetService("ReplicatedStorage").RigControllerEvent:FireServer("hit", Fast , 1 , "");
        
debug.setupvalue(_G.Attack,4,u7)
debug.setupvalue(_G.Attack,5,u8)
debug.setupvalue(_G.Attack,6,u9)
debug.setupvalue(_G.Attack,7,u10)

end

