spawn(function()
    pcall(function()
        while wait() do
            if _G.aufa and _G.Select_Weapon ~= nil then
                autofarm()
            elseif _G.aufa and _G.Select_Weapon == nil  then
                GUI:Notification{
                    Title = "AutoFarm",
                    Text = "SelectWeaponBeforeAutoFarm",
                    Duration = 3,
                    Callback = function() end
                }
                _G.aufa = false
                _G.AutoFarm = false
            end
        end
    end)
end)
local c = require(game.ReplicatedStorage.Util.CameraShaker)
c:Stop()
function autofarm()
    spawn(function()
        pcall(function()
            if game.Players.LocalPlayer.PlayerGui.Main.Quest.Visible == false then
                        _G.bring = false
                        f()
                        Tween(cq)
                        f()
                        wait(1)
                        if (cq.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 4 then
                            wait(1)
                            quest()
                            Quesss()
                        end
            elseif game.Players.LocalPlayer.PlayerGui.Main.Quest.Visible == true then
                for i,v in pairs(game:GetService("CollectionService"):GetTagged("ActiveRig")) do
                    f()
                    if  v.Name == _G.mm then
                    repeat wait()
                        if game.Players.LocalPlayer.PlayerGui.Main.Quest.Visible == true  and v.Humanoid.Health > 0 and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") then
                                f()
                               
                            if v.Name == _G.mm then
                                Farmtween = toTarget(v.HumanoidRootPart.Position,v.HumanoidRootPart.CFrame * CFrame.new(0,35,0))
                            end
                            if (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 200 and v.Name == _G.mm then
                                EquipWeapon(_G.Select_Weapon)
                                if Farmtween then  
                                    Farmtween:Stop()
                                end
                                _G.bring = true
                                set()
                                Quesss()
                                Buso()
                                ff = TP(v)
                                if ff then
                                    if Farmtween then  
                                        Farmtween:Stop()
                                    end
                                else
                                    ff = TP(v)
                                end
                            end
                        end
                    until _G.aufa == false or game.Players.LocalPlayer.PlayerGui.Main.Quest.Visible == false and game.Players.LocalPlayer.Character.Humanoid.Health <= 0 
                end
                end
            end
        end)
    end)
end
pcall(function()
local a = require(game.Players.LocalPlayer.PlayerScripts.CombatFramework)
    for i,v in pairs(debug.getupvalues(a)) do
        if type(v) == "table" then
            function fast()
                v.activeController:attack()
                v.activeController.hitboxMagnitude = 100
            end
        end
    end
end)
