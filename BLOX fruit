function can()
for i,v in pairs(game:GetService("Workspace").Enemies:GetDescendants()) do
    if v.ClassName == "MeshPart" then
        v.CanCollide = false
        end

end
end
function quest()
   if game.Players.LocalPlayer.Character.Humanoid.Health > 0 then

      local args = {
         [1] = "StartQuest",
         [2] = _G.T,
         [3] = _G.L
      }

      game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
   end
end
function Tween(a)
   local Distance = (a.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
   if Distance < 250 then
      Speed = 5000
   elseif Distance >= 250 then
      Speed = 300
   end
   game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart,TweenInfo.new(Distance/Speed, Enum.EasingStyle.Linear),{CFrame = a}):Play()
  wait(Distance/Speed)
end
function Twnon(e)
   local Distance = (e.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
   if Distance < 250 then
      Speed = 5000
   elseif Distance >= 250 then
      Speed = 300
   end
   game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart,TweenInfo.new(Distance/Speed, Enum.EasingStyle.Linear),{CFrame = e}):Play()
end

function pos()
    f()
    for _,x in pairs(game.Workspace.Enemies:GetDescendants()) do
        if x.Name == _G.mm then
           _G.pos =  x.HumanoidRootPart.CFrame
        end
    end
end
function o()
   if game.Players.LocalPlayer.PlayerGui.Main.Quest.Visible == true and game:GetService("Workspace").Enemies[_G.mm].Humanoid.Health > 0  then
      e = game:GetService("Workspace").Enemies[_G.mm].HumanoidRootPart.CFrame * CFrame.new(0,60,0)
      Tween(e)
    for o = 1,20 do
      for i,v in pairs(game.Workspace.Enemies:GetDescendants()) do
         if v.Name == _G.mm then
            if (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 250 and v.Humanoid.Health >= 0 then
            v.HumanoidRootPart.CFrame   =  _G.pos
            v.HumanoidRootPart.Size = Vector3.new(60,60,60)
            v.Humanoid.WalkSpeed = 1
            v.Head.CanCollide = false
			v.HumanoidRootPart.CanCollide = false
            v.Humanoid:ChangeState(11)
            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
            end
            end
         end
      end
   end
end

game:GetService("RunService").Heartbeat:Connect(function()
if _G.da then
   fastattack()
   end
end)




function king()
        if game:GetService("Workspace").NPCs:FindFirstChild("King Neptune") or game:GetService("Workspace").Enemies:FindFirstChild("Fishman Commando [Lv. 400]")  then
        
        
        elseif game.Players.LocalPlayer.Data.Level.Value >= 374 and game.Players.LocalPlayer.Data.Level.Value <= 449 and not game:GetService("Workspace").NPCs:FindFirstChild("King Neptune") and not  game:GetService("Workspace").Enemies:FindFirstChild("Fishman Commando [Lv. 400]") and not game:GetService("Workspace").Enemies:FindFirstChild("Fishman Warrior [Lv. 375]")  then
           game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Map.TeleportSpawn.EntrancePoint.CFrame 
        elseif game.Players.LocalPlayer.Data.Level.Value >= 449 and not game:GetService("Workspace").NPCs:FindFirstChild("King Neptune") and   game:GetService("Workspace").Enemies:FindFirstChild("Fishman Commando [Lv. 400]") and  game:GetService("Workspace").Enemies:FindFirstChild("Fishman Warrior [Lv. 375]")  then
        
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Map.TeleportSpawn.ExitPoint.CFrame 
        wait(2)
            end
end
spawn(function()
    pcall(function()
        while wait() do
            if _G.da then
        king()
        pos()
        end
        end
        end)
    end)
function Buso()
   if game.Players.LocalPlayer.Character:FindFirstChild("HasBuso") then
   else
      local buso = {
         [1] = "Buso"
      }

      game.ReplicatedStorage.Remotes.CommF_:InvokeServer(unpack(buso))
   end
end

function Quesss()
if Old_World then
    if  game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestReward.Title.Text == _G.TR then
else
local args = {
    [1] = "AbandonQuest"
}

game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))

    end
elseif  Three_World then
  
    end
end
function EquipWeapon(ToolSe)
    if game.Players.LocalPlayer.Backpack:FindFirstChild(ToolSe) then
        local tool = game.Players.LocalPlayer.Backpack:FindFirstChild(ToolSe)
        wait(.4)
        game.Players.LocalPlayer.Character.Humanoid:EquipTool(tool)
    end
end


_G.NoCIP = false
local function NoclipLoop()
        if game.Players.LocalPlayer.Character ~= nil and _G.NoCIP == true then
            for _, child in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                if child:IsA("BasePart") and child.CanCollide == true then
                    child.CanCollide = false 
                end
            end
        end
end
if _G.CNOCLIP then
    _G.CNOCLIP:Disconnect()
end
_G.CNOCLIP = game:GetService('RunService').Stepped:Connect(NoclipLoop)
function set()
if  game.Players.LocalPlayer.Character.Humanoid.Health > 0 then
   local args = {
      [1] = "SetSpawnPoint"
   }

   game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
end
end
