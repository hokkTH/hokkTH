if not game:IsLoaded() then
    game.Loaded:Wait()
end
if not game.Players.LocalPlayer then
    repeat wait() until game.Players.LocalPlayer
end
if not game.Players.LocalPlayer.character then
    repeat wait() until game.Players.LocalPlayer.Character
end
if game.PlaceId == 2753915549 or game.PlaceId == 4442272183 or  game.PlaceId == 7449423635 then
  loadstring(game:HttpGet("https://raw.githubusercontent.com/hokkTH/hokkTH/main/BLOX%20fruit"))()
elseif  game.PlaceId ==  1499872953 then
   loadstring(game:HttpGet("https://raw.githubusercontent.com/hokkTH/hokkTH/main/my%20hero.lua"))()
end
